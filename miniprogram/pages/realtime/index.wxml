  <view class="wrapper-realtime">
    <navBar 
      title='{{title}}'  
      background='#7dc5eb'></navBar>
    <map
      longitude="{{longitude}}" 
      latitude="{{latitude}}"
      show-compass="true"
      scale="16"
      markers="{{markers}}"
      polyline="{{polyline}}"
      style="width: 100%; height: 100vh;"></map>
      <view class="wrapper-realtime-add">
        <image bindtap="addMonitor" class="add" src="../../images/add.png"></image>
      </view>
  </view>
