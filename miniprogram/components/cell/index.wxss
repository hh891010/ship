.s-cell {
  display: flex;
  height: 100rpx;
  width: 100%;
  align-items: center;
  padding: 0 20rpx;
}
.s-cell.textarea {
  height: 300rpx;
}
.s-cell .titleicon {
  width: 60rpx;
  height: 60rpx;
  color: red;
  border: 1px solid red;
}
.s-cell .content {
  height: 100%;
  flex: 1;
  border-bottom: 1px solid #eeeeee;
  display: flex;
  align-items: center;
}
.s-cell .content .content-info {
  font-size: 28rpx;
  flex: 1;
  display: flex;
  align-items: center;
  height: 100%;
}
.s-cell .content .content-info .title {
  width: 48%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.s-cell .content .content-info .inputcls {
  width: 80%;
  text-align: right;
  color: #666666;
}
.s-cell .content .content-info .textareacls {
  border: 1px solid #EEEEEE;
  border-radius: 8rpx;
}
.s-cell .content .content-info .content-right {
  display: flex;
  height: 100%;
  width: 100%;
  align-items: center;
  justify-content: flex-end;
}
